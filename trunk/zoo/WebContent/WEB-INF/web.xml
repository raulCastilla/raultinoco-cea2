  </servlet>
  <servlet>
    <servlet-name>jirafa</servlet-name>
    <servlet-class>es.cea.JirafaServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>visita</servlet-name>
    <servlet-class>es.cea.Visita</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>leon</servlet-name>
    <servlet-class>es.cea.LeonServlet</servlet-class>
